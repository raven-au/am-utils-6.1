EOF
  eval "ac_cv_fs_header_$ac_fs_name=yes"
  break

else
  echo "$ac_t""no" 1>&6
fi
done

  echo $ac_n "checking for $ac_fs_name filesystem in <sys/fs/cachefs_fs.h>""... $ac_c" 1>&6
